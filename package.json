{
  "name": "composanator",
  "version": "3.0.3",
  "description": "Small library for composing functions together.",
  "main": "dist/composanator.js",
  "scripts": {
    "build": "rm -rf dist && babel --out-dir dist/ src/",
    "build:watch": "babel --watch --out-dir dist/ src/",
    "test": "./node_modules/.bin/mocha 'test/**/*.js' --compilers js:babel-register",
    "test:watch": "./node_modules/.bin/mocha 'test/**/*.js' --compilers js:babel-register --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Gwash3189/composanator.git"
  },
  "keywords": [
    "functional programming",
    "function",
    "composition",
    "functional",
    "micro"
  ],
  "author": "Adam Beck",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Gwash3189/composanator/issues"
  },
  "homepage": "https://github.com/Gwash3189/composanator",
  "devDependencies": {
    "babel": "^6.3.13",
    "babel-cli": "^6.3.17",
    "babel-eslint": "^4.1.6",
    "babel-preset-es2015": "^6.3.13",
    "babel-register": "^6.3.13",
    "chai": "~3.3.0",
    "eslint": "^1.10.3",
    "eslint-plugin-mocha": "^1.1.0",
    "mocha": "~2.3.3",
    "sinon": "^1.17.2"
  }
}
